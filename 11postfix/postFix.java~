public static double eval(string){
    tokens=s.split(" ");
    values=new stack<Integer>;
    for(int i=0;i<tokens.length;i++){
	if(isOp(tokens[i])){
	    //apply(op,b,a)
	    apply(tokens[i],values.pop(),values.pop());
	}
	else{
	    values.push(Integer.parseInt(tokens[i]));
	}
    }
    return values.pop();
}
